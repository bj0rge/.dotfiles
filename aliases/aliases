# MISC
alias cls=clear
alias mysql='mysql --auto-rehash'
alias tmux='tmux -2'
alias arc='PORT=4800 slc arc'
alias sshvbox="ssh -p 2222 node@217.128.13.95"
alias whatismyip='~/.dotfiles/bin/whatismyip'
alias helloworld='~/.dotfiles/bin/start.sh'
alias printcolors='~/.dotfiles/bin/printcolors.sh'
alias szsh="source ~/.zshrc"
alias a='vim ~/.dotfiles/aliases/aliases'
alias e='exit'

# HISTORY
alias hs='history | grep --color=auto'

# GIT
alias gs='git status'
alias gd='git diff'
alias ga='git add'
alias gA='git add -A'
alias gc='git commit -m'
alias gac='git commit -am'
alias gAc='git add -A && git commit -m'
alias gm='git merge'
alias gp='git push'
alias gconfpnp='git config user.name bjorge && git config user.email bastien.jorge@parknplug.fr'
alias gconf='git config user.name bj0rge && git config user.email bastien.jorge@gmail.com'

# CD
alias cd..='cd ..'
alias ..='cd ..'
alias ...='cd ../../'
alias ....='cd ../../../'
alias .....='cd ../../../../'

# LS
alias ls='ls $LS_OPTIONS --color=auto'
alias ll='ls $LS_OPTIONS -lAh'
alias l='ls $LS_OPTIONS -lh'
alias la='ls $LS_OPTIONS -A'


# THEFUCK
eval "$(thefuck --alias)"
